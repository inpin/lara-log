{
  "name": "inpin/lara-log",
  "description": "Add a middleware to capture all api calls and logs them.",
  "type": "package",
  "license": "MIT",
  "keywords": [
    "trait",
    "laravel",
    "laravel5",
    "eloquent",
    "logging",
    "log",
    "capture",
    "api",
    "api call",
    "catch"
  ],
  "authors": [
    {
      "name": "Esmaeel Cheshmehkhavar",
      "email": "ecw54321@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "illuminate/database": ">=5.0",
    "illuminate/support": ">=5.0",
    "laravel/framework": ">=5.6",
    "jenssegers/mongodb": "^3.4",
    "mongodb/mongodb": "^1.2"
  },
  "require-dev": {
    "orchestra/testbench": "^3.5",
    "orchestra/database": "^3.5",
    "phpunit/phpunit": "~7.0",
    "mockery/mockery": "~1.0",
    "fzaninotto/faker": "~1.4"
  },
  "autoload": {
    "psr-4": {
      "Inpin\\LaraLog\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Inpin\\LaraLog\\LaraLogServiceProvider"
      ]
    }
  }
}
